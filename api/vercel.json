{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/api" 
        }
    ]
}
